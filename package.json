{
  "name": "gnb-transfer",
  "version": "1.0.0",
  "type": "module",
  "description": "GNB Transfer - Full Stack Web Application",
  "main": "backend/server.mjs",
  "workspaces": [
    "mobile",
    "packages/*",
    "backend"
  ],
  "scripts": {
    "start": "node backend/server.mjs",
    "dev": "concurrently \"cd backend && nodemon server.mjs\" \"vite\"",
    "build": "npm ci && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "install-all": "npm install",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json}\"",
    "audit-fix": "npm audit fix --force",
    "preinstall": "echo 'Starting npm install...'",
    "mobile:start": "cd mobile && npm start",
    "mobile:ios": "cd mobile && npm run ios",
    "mobile:android": "cd mobile && npm run android",
    "mobile:build:ios": "cd mobile && eas build --platform ios",
    "mobile:build:android": "cd mobile && eas build --platform android"
  },
  "keywords": [
    "gnb",
    "transfer",
    "tourism",
    "react",
    "node",
    "express",
    "mongodb"
  ],
  "author": "GNB Transfer",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@sentry/react": "^10.30.0",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.3.0",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "fast-redact": "^3.5.0",
    "framer-motion": "^12.23.24",
    "helmet": "^8.1.0",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "minimatch": "^9.0.5",
    "mkdirp": "^3.0.1",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "openai": "^4.20.1",
    "pdfkit": "^0.17.2",
    "react": "^19.2.3",
    "react-big-calendar": "^1.19.4",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.2.3",
    "react-helmet": "^6.1.0",
    "react-i18next": "^14.1.0",
    "react-is": "^19.2.1",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "react-window": "^2.2.3",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.3.0",
    "redis": "^4.7.0",
    "regression": "^2.0.1",
    "sitemap": "^9.0.0",
    "socket.io-client": "^4.8.3",
    "stripe": "^15.5.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.11",
    "jest": "^29.7.0",
    "jsdom": "^27.3.0",
    "nodemon": "^3.1.4",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "vite": "^7.2.6",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "minimatch": "^9.0.5",
    "semver": "^7.6.3"
  },
  "exports": {
    ".": "./backend/server.mjs"
  }
}
