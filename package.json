{
  "name": "gnb-pro-fullstack",
  "version": "1.0.0",
  "description": "GNB Transfer - Full Stack Web Application",
  "main": "backend/server.mjs",
  "scripts": {
    "start": "node backend/server.mjs",
    "dev": "concurrently \"npm run dev --prefix backend\" \"vite\"",
    "build": "vite build",
    "preview": "vite preview",
    "install-all": "npm install && npm install --prefix backend",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json}\""
  },
  "keywords": [
    "gnb",
    "transfer",
    "tourism",
    "react",
    "node",
    "express",
    "mongodb"
  ],
  "author": "GNB Transfer",
  "license": "ISC",
  "dependencies": {
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.3.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^12.23.24",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.5.0",
