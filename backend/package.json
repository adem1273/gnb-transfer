{
  "name": "gnb-transfer-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "GNB Transfer Backend API",
  "main": "server.mjs",
  "scripts": {
    "start": "node server.mjs",
    "dev": "nodemon server.mjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint . --ext .mjs,.js,.ts",
    "lint:fix": "eslint . --ext .mjs,.js,.ts --fix",
    "typecheck": "tsc --noEmit",
    "seed": "node scripts/seed.mjs",
    "seed:users": "node scripts/seed.mjs --users",
    "seed:tours": "node scripts/seed.mjs --tours",
    "seed:reset": "node scripts/seed.mjs --reset",
    "db:indexes": "node scripts/create-indexes.mjs",
    "db:indexes:dry-run": "node scripts/create-indexes.mjs --dry-run",
    "db:indexes:drop-unused": "node scripts/create-indexes.mjs --drop-unused",
    "db:test-performance": "node scripts/test-query-performance.mjs"
  },
  "keywords": [
    "gnb",
    "transfer",
    "backend",
    "api",
    "express",
    "mongodb"
  ],
  "author": "GNB Transfer",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^10.30.0",
    "@sentry/profiling-node": "^10.30.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.4",
    "cloudinary": "^2.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "fast-redact": "^3.5.0",
    "form-data": "^4.0.5",
    "helmet": "^8.1.0",
    "i18next": "^23.7.6",
    "ioredis": "^5.3.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.3",
    "minimatch": "^9.0.5",
    "mkdirp": "^3.0.1",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "openai": "^6.15.0",
    "pdfkit": "^0.17.2",
    "prom-client": "^15.1.3",
    "redis": "^5.10.0",
    "regression": "^2.0.1",
    "sitemap": "^9.0.0",
    "socket.io": "^4.8.1",
    "stripe": "^15.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "minimatch": "^9.0.5",
    "semver": "^7.6.3"
  }
}
